      "source": [
        "#r \"nuget:LanguageExt.Core\"\n",
        "\n",
        "using LanguageExt;\n",
        "using static LanguageExt.Prelude;\n",
        "\n",
        "Func<int, bool> isOdd = x => x % 2 == 1;\n",
        "var value = Some(11);\n",
        "var result = value\n",
        "    .Select(x => x* 3)\n",
        "    .Select(isOdd)\n",
        "    .Match(x => x, () => false);\n",
        "result\n",
        "\n",
        "\n",
        ""
      ],
